<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="composeTitle">Title</label>
    <input type="text" class="form-control" name="composeTitle">
  </div>
  <div class="form-group">
    <label for="composePost">Post</label>
    <textarea class="form-control" rows="6" name="composePost"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="composeSubmit">Publish</button>
</form>
<%- include('partials/footer'); -%>
